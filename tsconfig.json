{
    "compilerOptions": {
      "target": "es6",
      "module": "esnext",
      "strict": true,
      "jsx": "react",
      "esModuleInterop": true,
      "moduleResolution": "node",
      "skipLibCheck": true,
      "forceConsistentCasingInFileNames": true,
      "baseUrl": "src",
      "paths": {
        "*": ["src/*", "*"]
      }
    },
    "include": ["src"],
    "exclude": ["node_modules"]
  }
  